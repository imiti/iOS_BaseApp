platform :ios, "8.0"
use_frameworks!


def pods_base
    
    pod 'MaterialKit', '~> 0.3.5'
    pod 'SwiftyJSON', '~> 2.2.0'
    pod 'IJReachability', :git => 'https://github.com/Isuru-Nanayakkara/IJReachability.git'
    
end


target 'BaseAppiOS' do
    pods_base
end

target 'BaseAppiOSTests' do
    pods_base
end